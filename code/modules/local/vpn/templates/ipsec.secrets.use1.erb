# ipsec.secrets.use1.erb
: RSA	{
	# RSA 2192 bits   <%= scope.lookupvar('::hostname_full') %> <%= scope.lookupvar('::serial') %>
	# for signatures only, UNSAFE FOR ENCRYPTION
	#pubkey=<%= scope.lookupvar('source_key') %>
	Modulus: <%= scope.lookupvar('source_modulus') %>
	PublicExponent: <%= scope.lookupvar('source_public_exponent') %> 
	# everything after this point is CKA_ID in hex format when using NSS
	PrivateExponent: <%= scope.lookupvar('source_private_exponent') %> 
	Prime1: <%= scope.lookupvar('source_prime1') %> 
	Prime2: <%= scope.lookupvar('source_prime2') %> 
	Exponent1: <%= scope.lookupvar('source_exponent1') %> 
	Exponent2: <%= scope.lookupvar('source_exponent2') %> 
	Coefficient: <%= scope.lookupvar('source_coefficient') %> 
	CKAIDNSS: <%= scope.lookupvar('source_ckaidnss') %> 
	}
# do not change the indenting of that "}"

