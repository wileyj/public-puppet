# ipsec.secrets.usw1.erb
: RSA	{
	# RSA 2192 bits   <%= scope.lookupvar('::hostname_full') %> <%= scope.lookupvar('::serial') %>
	# for signatures only, UNSAFE FOR ENCRYPTION
	#pubkey=<%= scope.lookupvar('dest_key') %>
	Modulus: <%= scope.lookupvar('dest_modulus') %>
	PublicExponent: <%= scope.lookupvar('dest_public_exponent') %> 
	# everything after this point is CKA_ID in hex format when using NSS
	PrivateExponent: <%= scope.lookupvar('dest_private_exponent') %> 
	Prime1: <%= scope.lookupvar('dest_prime1') %> 
	Prime2: <%= scope.lookupvar('dest_prime2') %> 
	Exponent1: <%= scope.lookupvar('dest_exponent1') %> 
	Exponent2: <%= scope.lookupvar('dest_exponent2') %> 
	Coefficient: <%= scope.lookupvar('dest_coefficient') %> 
	CKAIDNSS: <%= scope.lookupvar('dest_ckaidnss') %> 
	}
# do not change the indenting of that "}"

