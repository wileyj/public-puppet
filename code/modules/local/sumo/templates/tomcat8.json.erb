{
    "api.version": "v1",
    "source": {
        "name": "Tomcat8 Catalina Log",
        "hostName": "<%= scope.lookupvar('::hostname_full') %>",
        "sourceType": "LocalFile",
        "automaticDateParsing": true,
        "multilineProcessingEnabled": false,
        "useAutolineMatching": true,
        "forceTimeZone": false,
        "timeZone": "America/New_York",
        "defaultDateFormat": "dd/MMM/yyyy HH:mm:ss",
        "category": "Service/<%= scope.lookupvar('::tag_environment') %>/tomcat",
        "pathExpression": "/var/log/tomcat8/catalina.out"
    }
}