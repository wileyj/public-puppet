{
    "checks": {
        "vpn_to_nat_001_usw2": {
            "command": "/etc/sensu/plugins/check-socket.pl -h platform-nat-001.p.usw2.local.com -p 22 ",
            "wiki": "http://wiki.something/for/check/failure",
            "interval": 30,
            "occurrences": 4,
            "handlers": [
                "default",
                "remediation",
                "mailer"
            ],
            "occurrences": 4,
            "refresh": 10,
            "standalone": false,
            "subscribers": [
                "vpn_to_nat_001_usw2"
            ],
            "remediation": {
                "remediation_ipsec": {
                    "occurrences": [
                        "1-5"
                    ],
                    "severities": [
                        2
                    ]
                }
            }
        },
        "remediation_ipsec": {
            "command": "sudo /etc/init.d/ipsec restart",
            "subscribers": [],
            "handlers": [
                "mailer"
            ],
            "publish": false
        }
    }
}