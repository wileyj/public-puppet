# This file is autogenerated by Puppet. Manual changes will be overwritten!
[program:<%= @name -%>]
command=<%= @command %>
<% if @numprocs.to_i > 1 -%>
process_name=%(program_name)s_%(process_num)02d
<% end -%>
numprocs=<%= @numprocs %>
numprocs_start=<%= @numprocs_start %>
priority=<%= @priority %>
autostart=<%= @autostart %>
autorestart=<%= @autorestart %>
startsecs=<%= @startsecs %>
startretries=<%= @retries %>
exitcodes=<%= @exitcodes %>
stopsignal=<%= @stopsignal %>
stopwaitsecs=<%= @stopwait %>
stopasgroup=<%= @stopasgroup %>
<% if @directory -%>
directory=<%= @directory %>
<% end -%>
<% if @user -%>
user=<%= @user %>
<% end -%>
redirect_stderr=<%= @redirect_stderr %>
<% if @stdout_logfile -%>
stdout_logfile=<%= @stdout_logfile %>
<% elsif @setup_logging -%>
stdout_logfile=/var/log/supervisor/<%= @name -%>/<%= @name -%>.out
<% end -%>
<% if @setup_logging -%>
stdout_logfile_maxbytes=<%= @stdout_logfile_maxsize %>
stdout_logfile_backups=<%= @stdout_logfile_keep %>
<% end -%>
<% if @stderr_logfile -%>
stderr_logfile=<%= @stderr_logfile %>
<% elsif @setup_logging -%>
stderr_logfile=/var/log/supervisor/<%= @name -%>/<%= @name -%>.err
<% end -%>
<% if @setup_logging -%>
stderr_logfile_maxbytes=<%= @stderr_logfile_maxsize %>
stderr_logfile_backups=<%= @stderr_logfile_keep %>
<% end -%>
<% if @environment -%>
environment=<%= @environment %>
<% end -%>
<% if @umask %>
umask=<%= @umask %>
<% end -%>
